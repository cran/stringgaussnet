	month = mar,
	year = {2015},
	keywords = {Biological sciences, Developmental biology, Genetics, Neuroscience},
	file = {Full Text PDF:/home/emmanuel/.mozilla/firefox/t95f1nok.default/zotero/storage/E83XXH6H/Cotney et al. - 2015 - The autism-associated chromatin modifier CHD8 regu.pdf:application/pdf;Snapshot:/home/emmanuel/.mozilla/firefox/t95f1nok.default/zotero/storage/3Z94UCQC/ncomms7404.html:text/html}
