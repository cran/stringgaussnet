	pmid = {24943223},
	keywords = {Animals, Apolipoproteins E, Atherosclerosis, Macrophages, Male, Metabolomics, Mice, Knockout, Plaque, Atherosclerotic, Tissue Inhibitor of Metalloproteinase-3},
	pages = {438--443}
}

@article{reveille_genome-wide_2010,
	title = {Genome-wide association study of ankylosing spondylitis identifies non-{MHC} susceptibility loci},
	volume = {42},
	issn = {1061-4036},
