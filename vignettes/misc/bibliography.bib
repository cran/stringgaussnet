	month = apr,
	year = {2015},
	keywords = {Bioinformatics, Biological sciences, Cancer},
	file = {Full Text PDF:/home/emmanuel/.mozilla/firefox/t95f1nok.default/zotero/storage/C49V8ZHH/Verfaillie et al. - 2015 - Decoding the regulatory landscape of melanoma reve.pdf:application/pdf;Snapshot:/home/emmanuel/.mozilla/firefox/t95f1nok.default/zotero/storage/Q3ZNCU5C/ncomms7683.html:text/html}
