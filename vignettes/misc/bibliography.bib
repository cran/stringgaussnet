	keywords = {Adult, Blood Proteins, Female, Gene Expression, Gene Expression Profiling, Humans, Male, Membrane Proteins, Middle Aged, Nerve Tissue Proteins, Oligonucleotide Array Sequence Analysis, Osteonectin, Spondylarthropathies},
	pages = {R168}
