	keywords = {Adult, Case-Control Studies, Female, Genetic Linkage, Genetic Predisposition to Disease, Genetic Testing, Genotype, Humans, Male, Polymorphism, Single Nucleotide, Spondylarthritis, Tenascin},
	pages = {1655--1658}
}

