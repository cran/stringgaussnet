@article{duan_fcgr2b_2012,
	title = {The {FCGR}2B rs10917661 polymorphism may confer susceptibility to ankylosing spondylitis in {Han} {Chinese}: a case-control study},
	volume = {41},
	issn = {1502-7732},
	shorttitle = {The {FCGR}2B rs10917661 polymorphism may confer susceptibility to ankylosing spondylitis in {Han} {Chinese}},
	doi = {10.3109/03009742.2011.625972},
	abstract = {OBJECTIVE: Single-nucleotide polymorphisms (SNPs) in the Fc gamma receptor IIB (FCGR2B) gene have recently been found to be associated with several human autoimmune diseases. We undertook the current study to investigate the influence of these polymorphisms on the risk of ankylosing spondylitis (AS).
METHOD: A total of 306 patients with AS from Anhui, China, fulfilling the modified New York Criteria, and 300 matched healthy controls were analysed. All subjects were genotyped for two SNPs (rs1050501, rs10917661) in the FCGR2B gene, and the SNaPshot Assay was used for genotyping.
RESULTS: SNP rs10917661 was significantly associated with AS [C vs. T: odds ratio (OR) 1.723, 95\% confidence interval (CI) 1.086-2.733, p = 0.020; genotype: p = 0.026] whereas no association was found for rs1050501. Furthermore, no haplotype was found to be associated with AS.
CONCLUSION: These findings indicated that rs10917661 may be a novel SNP involved in AS genetic predisposition in the Han Chinese population.},
	language = {eng},
	number = {3},
	journal = {Scandinavian Journal of Rheumatology},
	author = {Duan, Z.-H. and Pan, F.-M. and Zeng, Z. and Zhang, T.-C. and Wang, S. and Li, G.-X. and Mei, Y. and Gao, J. and Ge, R. and Ye, D.-Q. and Zou, Y.-F. and Xu, S.-Q. and Xu, J.-H. and Zhang, L.},
