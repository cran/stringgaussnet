	month = jan,
	year = {2005},
	pmid = {16284200},
	pages = {e175--e175},
	file = {Full Text PDF:/home/emmanuel/.mozilla/firefox/t95f1nok.default/zotero/storage/BFJ2SWXB/Dai et al. - 2005 - Evolving genetranscript definitions significantly.pdf:application/pdf;Snapshot:/home/emmanuel/.mozilla/firefox/t95f1nok.default/zotero/storage/S3SKF3CF/e175.html:text/html}
