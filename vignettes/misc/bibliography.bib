	number = {3},
	journal = {The Journal of Clinical Investigation},
	author = {He, Miao and Kratz, Lisa E. and Michel, Joshua J. and Vallejo, Abbe N. and Ferris, Laura and Kelley, Richard I. and Hoover, Jacqueline J. and Jukic, Drazen and Gibson, K. Michael and Wolfe, Lynne A. and Ramachandran, Dhanya and Zwick, Michael E. and Vockley, Jerry},
