	keywords = {Animals, beta 2-Microglobulin, Cells, Cultured, Dendritic Cells, Disease Susceptibility, HLA-B27 Antigen, Male, Phenotype, Rats, Rats, Inbred Lew, Rats, Transgenic, Spondylarthritis},
